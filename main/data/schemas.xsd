					<xsd:annotation>
						<xsd:documentation>
							Идентификатор организации в ЕСИА (ЮЛ / ИП)
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="organization" type="tns:orgType">
					<xsd:annotation>
						<xsd:documentation>
